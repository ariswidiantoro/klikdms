CREATE FUNCTION adjustment_stock() RETURNS trigger
    LANGUAGE plpgsql
    AS $$DECLARE tgl timestamp;
DECLARE cbid VARCHAR(8);
DECLARE noslip VARCHAR(14);
DECLARE kodeBarang VARCHAR(50);
DECLARE jumlah decimal;
DECLARE stock decimal;
DECLARE qtyadjustment decimal;
DECLARE hpp numeric;
DECLARE debit numeric;
DECLARE totalhpp numeric;
DECLARE saldo numeric;
DECLARE totalsaldo numeric;
BEGIN

	SELECT adj_cbid INTO cbid FROM spa_adjustment WHERE adjid = NEW.dadj_adjid;
	SELECT inve_qty,inve_hpp,inve_saldo, inve_kode INTO jumlah,hpp,saldo,kodeBarang FROM spa_inventory WHERE inveid = NEW.dadj_inveid FOR UPDATE;
    
	qtyadjustment = NEW.dadj_plus-NEW.dadj_minus;
	debit = qtyadjustment * NEW.dadj_hpp;
	
	stock = jumlah + qtyadjustment;
	totalhpp = hpp;
	totalsaldo = saldo+debit;
	
	IF stock < 0 THEN
            RAISE EXCEPTION USING MESSAGE = 'Stock untuk kode barang ' || kodeBarang || ' tidak mencukupi, Stock saat ini = '||jumlah;
    ELSEIF stock > 0 THEN
		totalhpp = ((jumlah*hpp)+ debit) / (jumlah+qtyadjustment);
	ELSE
		totalsaldo = 0;
	END IF;
	
	UPDATE spa_inventory SET inve_qty = stock, inve_hpp = totalhpp,
	inve_saldo = totalsaldo WHERE inveid = NEW.dadj_inveid;


	INSERT INTO spa_kartu_stock(
	ks_tgl, ks_cbid, ks_notrans,ks_inveid, ks_in, ks_out, ks_total, ks_type, ks_harga, ks_hpp, ks_debit, ks_kredit, ks_saldo)
	VALUES(now(), cbid,NEW.dadj_adjid, NEW.dadj_inveid, NEW.dadj_plus,NEW.dadj_minus, stock,
	'AS',NEW.dadj_hpp, totalhpp, NEW.dadj_hpp*NEW.dadj_plus, NEW.dadj_hpp*NEW.dadj_minus, totalsaldo);
	RETURN NEW;
END;$$;


