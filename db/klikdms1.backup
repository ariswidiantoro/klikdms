    LANGUAGE plpgsql
    AS $$DECLARE tgl timestamp;
DECLARE cbid VARCHAR(8);
DECLARE noslip VARCHAR(14);
DECLARE kodeBarang VARCHAR(50);
DECLARE suppel VARCHAR(12);
DECLARE jumlah decimal;
DECLARE stock decimal;
DECLARE hpp numeric;
DECLARE kredit numeric;
DECLARE totalhpp numeric;
DECLARE saldo numeric;
BEGIN

	SELECT trbr_cbid, trbr_supid INTO cbid, suppel FROM spa_retbeli LEFT JOIN  spa_trbr ON trbrid = rb_trbrid WHERE rbid = NEW.detb_rbid;
	SELECT inve_qty,inve_hpp,inve_saldo,inve_kode INTO jumlah,hpp,saldo,kodeBarang FROM spa_inventory WHERE inveid = NEW.detb_inveid FOR UPDATE;
    
	kredit = NEW.detb_subtotal/1.1;
	
	
	stock = jumlah - NEW.detb_qty;
	if stock = 0 THEN
		saldo = kredit;
		totalhpp = 0;
    ELSE
		totalhpp = ((jumlah*hpp)- kredit) / (jumlah-NEW.detb_qty);
	END IF;
	IF stock < 0 THEN
            RAISE EXCEPTION USING MESSAGE = 'Stock untuk kode barang ' || kodeBarang || ' tidak mencukupi';
    END IF;
	
	UPDATE spa_inventory SET inve_qty = stock, inve_hpp = totalhpp, inve_saldo = (saldo-kredit) WHERE inveid = NEW.detb_inveid;


	INSERT INTO spa_kartu_stock(
	ks_tgl, ks_cbid, ks_notrans, ks_suppel,ks_inveid, ks_out, ks_total, ks_type, ks_harga, ks_hpp, ks_kredit, ks_saldo)
